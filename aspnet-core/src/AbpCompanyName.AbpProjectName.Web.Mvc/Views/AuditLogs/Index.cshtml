@using AbpCompanyName.AbpProjectName.Web.Startup
@{
    ViewBag.Title = L("AuditLogs");
    ViewBag.CurrentPageName = PageNames.AuditLogs;
}
@section scripts
{
    <environment names="Development">
        <script src="~/view-resources/Views/AuditLogs/Index.js" asp-append-version="true"></script>
    </environment>

    <environment names="Staging,Production">
        <script src="~/view-resources/Views/AuditLogs/Index.min.js" asp-append-version="true"></script>
    </environment>
}
<section class="content-header">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-6">
                <h1>@L("AuditLogs")</h1>
            </div>
            <div class="col-sm-6">
            </div>
        </div>
    </div>
</section>
<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <div class="row">
                            <div class="col-md-6">
                                <!--Use for bulk actions-->
                            </div>
                            <div class="col-md-6">
                                @await Html.PartialAsync("~/Views/AuditLogs/Index.AdvancedSearch.cshtml")
                            </div>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table id="AuditLogsTable" class="table table-striped table-bordered">
                                <thead>
                                    <tr>
                                        <th>ID</th>
                                        <th>@L("ExecutionTime")</th>
                                        <th>@L("UserName")</th>
                                        <th>@L("ServiceName")</th>
                                        <th>@L("MethodName")</th>
                                        <th>@L("ExecutionDuration")</th>
                                        <th>@L("ClientIpAddress")</th>
                                        <th>@L("ClientName")</th>
                                        <th>@L("BrowserInfo")</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>